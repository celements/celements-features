  </property>
  <property name="eintritt" type="string">
    <column name="MCR_EINTRITT" length="255"/>
  </property>
  <property name="austritt" type="string">
    <column name="MCR_AUSTRITT" length="255"/>
  </property>
  <property name="ehepartner" type="string">
    <column name="MCR_EHEPARTNER" length="255"/>
  </property>
  <property name="email_geschaeft" type="string">
    <column name="MCR_EMAIL_GESCHAEFT" length="255"/>
  </property>
  <property name="email_privat" type="string">
    <column name="MCR_EMAIL_PRIVAT" length="255"/>
  </property>
  <property name="fax_geschaeft" type="string">
    <column name="MCR_FAX_GESCHAEFT" length="255"/>
  </property>
  <property name="fax_privat" type="string">
    <column name="MCR_FAX_PRIVAT" length="255"/>
  </property>
  <property name="firmenname_geschaeft" type="string">
    <column name="MCR_FIRMENNAME_GESCHAEFT" length="255"/>
  </property>
  <property name="geburtsdatum" type="string">
    <column name="MCR_GEBURTSDATUM" length="255"/>
  </property>
  <property name="klassifikation" type="string">
    <column name="MCR_KLASSIFIKATION" length="255"/>
  </property>
  <property name="mobile_geschaeft" type="string">
    <column name="MCR_MOBILE_GESCHAEFT" length="255"/>
  </property>
  <property name="mobile_privat" type="string">
    <column name="MCR_MOBILE_PRIVAT" length="255"/>
  </property>
  <property name="ort_geschaeft" type="string">
    <column name="MCR_ORT_GESCHAEFT" length="255"/>
