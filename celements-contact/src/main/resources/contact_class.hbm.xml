<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="Celements.ContactClass" table="cel_contactclass">
  <id name="id" type="long" unsaved-value="undefined">
    <column name="CON_ID" not-null="true" />
    <generator class="assigned" />
  </id>
  <property name="title" type="string">
    <column name="CON_TITLE" length="64"/>
  </property>
  <property name="firstname" type="string">
    <column name="CON_FIRST_NAME" length="128" />
  </property>
  <property name="lastname" type="string">
    <column name="CON_LAST_NAME" length="128"/>
  </property>
  <property name="sex" type="string">
    <column name="CON_SEX" length="16"/>
  </property>
</class>

<class entity-name="Celements.ContactAddressClass" table="cel_contactaddressclass">
  <id name="id" type="long" unsaved-value="undefined">
    <column name="COA_ID" not-null="true" />
    <generator class="assigned" />
  </id>
  <property name="street_nr" type="string">
    <column name="COA_STREET_NR" length="255"/>
  </property>
  <property name="zip" type="string">
    <column name="COA_ZIP" length="32" />
  </property>
  <property name="city" type="string">
    <column name="COA_CITY" length="128"/>
  </property>
  <property name="country" type="string">
    <column name="COA_COUNTRY" length="64"/>
  </property>
</class>

