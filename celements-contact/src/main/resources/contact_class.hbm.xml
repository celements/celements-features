  </property>
  <property name="strasse_geschaeft" type="string">
    <column name="MCR_STRASSE_GESCHAEFT" length="255"/>
  </property>
  <property name="strasse_privat" type="string">
    <column name="MCR_STRASSE_PRIVAT" length="255"/>
